<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
 
 <filter>
 	<filter-name>endocingfilter</filter-name>
 	<filter-class>com.xb.filter.EncodingFilter</filter-class>
 </filter>
 <filter-mapping>
 	<filter-name>endocingfilter</filter-name>
 	<url-pattern>/*</url-pattern>
 </filter-mapping>
 
  <servlet>
    <servlet-name>CashCouponServlet</servlet-name>
    <servlet-class>com.xb.wap.CashCouponServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SaveUserInfoAction</servlet-name>
    <servlet-class>com.xb.wap.SaveUserInfoAction</servlet-class>
  </servlet>
  

<servlet>
    <servlet-name>SaveUserInfoIPadAction</servlet-name>
    <servlet-class>com.xb.wap.SaveUserInfoIPadAction</servlet-class>
  </servlet>
<servlet-mapping>
    <servlet-name>SaveUserInfoIPadAction</servlet-name>
    <url-pattern>/savepaduserinfo.xhtml</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>CashCouponServlet</servlet-name>
    <url-pattern>/cashcoupon.xhtml</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaveUserInfoAction</servlet-name>
    <url-pattern>/saveuserinfo.xhtml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IndexAction</servlet-name>
    <servlet-class>com.xb.wap.IndexAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexAction</servlet-name>
    <url-pattern>/index.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IpadAction</servlet-name>
    <servlet-class>com.xb.wap.IpadAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IpadAction</servlet-name>
    <url-pattern>/ipad.html</url-pattern>
  </servlet-mapping>	
  <servlet>
    <servlet-name>IphoneAction</servlet-name>
    <servlet-class>com.xb.wap.IphoneAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IphoneAdAction</servlet-name>
    <servlet-class>com.xb.wap.IphoneAdAction</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>IphoneAction</servlet-name>
    <url-pattern>/iphone.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IphoneAdAction</servlet-name>
    <url-pattern>/1.html</url-pattern>
  </servlet-mapping>	
  <servlet>
    <servlet-name>IphoneFormAction</servlet-name>
    <servlet-class>com.xb.wap.IphoneFormAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>VisitCountAction</servlet-name>
    <servlet-class>com.xb.wap.VisitCountAction</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>IphoneFormAction</servlet-name>
    <url-pattern>/2.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VisitCountAction</servlet-name>
    <url-pattern>/visitCount.html</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
